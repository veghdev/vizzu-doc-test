{
  "name": "vizzu-story",
  "version": "0.4.0",
  "description": "Build and present animated data stories",
  "main": "dist/vizzu-story.min.js",
  "types": "dist/vizzu-story.d.ts",
  "type": "module",
  "files": ["dist/vizzu-story.d.ts"],
  "scripts": {
    "test": "NODE_OPTIONS='--no-warnings --experimental-vm-modules' npx jest --verbose",
    "prepare": "husky install",
    "check": "npm-run-all check-dts check-prettier check-eslint test",
    "check-dts": "check-dts src/vizzu-story.d.ts",
    "prettier": "prettier -w src/ .github/",
    "check-prettier": "prettier -c src/ .github/",
    "eslint": "eslint --ext .js,.cjs,.mjs -c .eslintrc.cjs --fix src/",
    "check-eslint": "eslint --ext .js,.cjs,.mjs -c .eslintrc.cjs src/",
    "rollup": "rm -rf dist && rollup -c",
    "pack": "rm -rf build && mkdir -p build && npm pack --pack-destination build && tar -ztvf build/*.tgz",
    "build": "npm-run-all rollup pack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vizzuhq/vizzu-story-js.git"
  },
  "keywords": [
    "template",
    "interactive",
    "presentation",
    "data-visualization",
    "charting",
    "vizzu"
  ],
  "author": "Vizzu Inc.",
  "license": "Apache-2.0",
  "homepage": "https://vizzu-story.vizzuhq.com/",
  "url": "https://github.com/vizzuhq/vizzu-story-js/issues",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "check-dts": "^0.7.1",
    "prettier": "^2.6.2",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "rollup": "^2.70.2",
    "@rollup/plugin-terser": "^0.4.3",
    "rollup-plugin-copy": "^3.4.0",
    "typedoc": "~0.23.24",
    "typedoc-plugin-markdown": "~3.14.0",
    "typedoc-plugin-rename-defaults": "~0.6.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vizzu": "~0.7.0"
  }
}
